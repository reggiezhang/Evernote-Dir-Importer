{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "rules": {
        "max-len": [
            "error",
            120,
            4
        ],
        "require-jsdoc": "off",
        "no-console": "off"
    }
}
